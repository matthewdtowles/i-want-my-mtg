{
  "name": "i-want-my-mtg",
  "version": "0.0.1",
  "description": "MTG Collection Tracker",
  "author": "Matthew Towles",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "chown -R $(whoami):$(whoami) ./dist",
    "start:fresh": "npx tailwindcss -i ./src/http/styles/tailwind.css -o ./src/http/public/css/tailwind.css && nest build && npm run start",
    "build": "nest build",
    "build:css": "npx tailwindcss -i ./src/http/styles/tailwind.css -o ./src/http/public/css/tailwind.css",
    "build:test-views": "rm -rf test/http/views && cp -r src/http/views test/http/ && ls -l test/http/views",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "NODE_OPTIONS='-r tsconfig-paths/register' typeorm-ts-node-commonjs",
    "scry:watch": "cd scry && cargo run -- watch",
    "scry:cards": "cd scry && cargo run -- cards",
    "scry:prices": "cd scry && cargo run -- prices",
    "scry:archive": "cd scry && cargo run -- archive",
    "scry:sets": "cd scry && cargo run -- sets",
    "scry:clarity": "cd scry && cargo run -- clarity",
    "scry:build": "cd scry && cargo build --release",
    "scry:test": "cd scry && cargo test"
  },
  "dependencies": {
    "@lifeart/ember-language-server": "^2.18.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/typeorm": "^10.0.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "express-handlebars": "^7.1.2",
    "handlebars": "^4.7.8",
    "nestjs-command": "^3.1.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "stream-json": "^1.9.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.4",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/stream-chain": "^2.1.0",
    "@types/stream-json": "^1.7.8",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/main.ts",
      "!src/**/index.ts",
      "!src/**/test-utils.ts",
      "!src/**/mock.*.ts",
      "!src/**/*dto*.ts",
      "!src/**/*entity*.ts",
      "!src/**/*module*.ts",
      "!src/**/*port*.ts"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.spec.ts"
    ]
  }
}
